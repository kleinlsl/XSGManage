<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 角度 corners为圆角-->
    <corners android:radius="35dp"/>

    <!-- 填充色 -->
    <solid android:color="#ffffff"/>

    <!-- 填充设置 -->
    <padding android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp"/>

    <!-- 描边 设置边框线宽及颜色 -->
    <stroke android:color="#cccacb"
        android:width="1dp"/>
</shape>